{
	"name": "Music List",
	"version": "1.0.3",
	"description": "NoDMCA music-list with spotify api",
	"keywords": [
		"ejs",
		"spotify",
		"express",
		"javascript",
		"postcss",
		"tailwind",
		"babel"
	],
	"type": "module",
	"author": "fxhxyz <fxhxyz@proton.me> (https://fxhxyz.vercel.app/)",
	"homepage": "https://music-list-lol.onrender.com/",
	"bugs": "https://github.com/fxhxyz4/MusicList/issues",
	"scripts": {
		"dev": "nodemon index.js -- --config",
		"server": "node index.js",
		"ejs": "ejs ./views/index.ejs -o ./public/index.html",
		"css": "css-minify -f ./public/styles/global.css -o ./public/styles/",
		"babel:watch": "babel public/scripts/**/*.js -d public/out --presets=@babel/preset-env -w",
		"postcss:watch": "postcss ./public/styles/global.css -o ./public/styles/style.css -w",
		"tailwind:watch": "tailwindcss -i ./public/styles/global.css -o ./public/styles/global.css -w",
		"webpack": "webpack",
		"prod": "npm run ejs && npm run tailwind:prod && npm run css && npm run babel:prod",
		"babel:prod": "babel public/scripts/**/*.js -d public/out --presets=@babel/preset-env",
		"tailwind:prod": "tailwindcss -i ./public/styles/global.css -o ./public/styles/global.css"
	},
	"dependencies": {
		"cors": "^2.8.5",
		"css-minify": "^2.0.0",
		"dotenv": "^16.0.3",
		"ejs": "^3.1.9",
		"express": "^4.18.2",
		"express-session": "^1.17.3"
	},
	"devDependencies": {
		"@babel/cli": "^7.21.0",
		"@babel/core": "^7.21.4",
		"autoprefixer": "^10.4.14",
		"colors": "^1.4.0",
		"nodemon": "^2.0.22",
		"postcss": "^8.4.21",
		"postcss-cli": "^10.1.0",
		"prettier": "^2.8.7",
		"tailwindcss": "^3.2.7",
		"webpack": "^5.77.0",
		"webpack-cli": "^5.0.1"
	},
	"engines": {
		"node": "18.x"
	}
}
