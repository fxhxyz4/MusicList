{
	"version": 2,
	"name": "music-list",
	"public": true,
	"cleanUrls": true,
	"trailingSlash": false,
	"devCommand": "npm run dev",
	"installCommand": "npm install",
	"builds": [
		{
			"src": "./index.js",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "/"
		}
	],
	"env": {
		"PORT": 3939,
		"SESSION_SECRET": "'your session secret'",
		"TWITCH_ID": "YOUR_CLIENT_ID",
		"AUTH_URL": "https://id.twitch.tv/oauth2/authorize"
	}
}
